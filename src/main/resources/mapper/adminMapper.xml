<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dhu.dao.AdminDao">
    <select id="adminSelectById" parameterType="String" resultType="Admin">
        select *
        from admin
        where adminId = #{adminId}
    </select>
</mapper>